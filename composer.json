{
  "name": "sav/hydrator-creator",
  "description": "Creates an object and hydrates it with data",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Starikov Andrey",
      "email": "sparhokm@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.31",
    "friendsofphp/php-cs-fixer": "^3.17.0",
    "phpunit/phpunit": "^10.1.3",
    "psalm/plugin-phpunit": "^0.18.4",
    "symfony/var-dumper": "^6.3.0",
    "vimeo/psalm": "^5.12.0"
  },
  "autoload": {
    "psr-4": {
      "Sav\\Hydrator\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    },
    "sort-packages": true
  },
  "scripts": {
    "fixcs": "PHP_CS_FIXER_IGNORE_ENV=1 vendor/bin/php-cs-fixer fix -v --diff --allow-risky=yes",
    "psalm": "psalm --show-info=true --no-diff ",
    "test": "phpunit"
  }
}